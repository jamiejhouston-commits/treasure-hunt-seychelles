{
  "name": "treasure-of-seychelles",
  "version": "1.0.0",
  "description": "NFT treasure hunt on XRPL featuring Olivier Levasseur's lost pirate treasure",
  "main": "index.js",
  "scripts": {
    "generate": "cd scripts && node generate_art.js",
    "metadata": "cd scripts && node build_metadata.js",
    "upload": "cd scripts && node upload_ipfs.js",
    "mint:testnet": "cd scripts && node mint_testnet.js",
    "test": "node scripts/testSuite.js",
    "deploy": "node deploy.js",
    "test:art": "node scripts/generateArt.js --test",
    "test:metadata": "node scripts/buildMetadata.js --test",
    "test:ipfs": "node scripts/uploadIPFS.js --test",
    "test:xrpl": "node scripts/batchMint.js --test",
    "install:all": "npm install && npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm run serve",
    "migrate": "cd backend && npm run migrate",
    "seed": "cd backend && npm run seed",
    "db:import": "cd backend && node seeds/importNFTs.js",
    "generate:ch6:premint": "node scripts/render_ch6_manifest_cards.mjs",
    "generate:ch6:premint:legacy": "node scripts/build_ch6_cards_from_examples.cjs"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "nft.storage": "^7.1.1",
    "sqlite3": "^5.1.7",
    "xrpl": "^2.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sharp": "^0.34.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seychelles-treasure/nft-collection.git"
  },
  "keywords": [
    "nft",
    "xrpl",
    "treasure-hunt",
    "seychelles",
    "pirate",
    "levasseur"
  ],
  "author": "Treasure of Seychelles Team",
  "license": "MIT"
}
